var _second = 1000;          /* opredelyaem razmernost razryadov*/
var _minute = _second * 60;
var _hour = _minute * 60;
var _day = _hour * 24;
var timer;
    function showRemaining() {
        var now = new Date();
        var distance = end - now;
        if (distance < 0) {
            document.getElementById("countdown").innerHTML = "Акция окончена";
            return;
        }
        var days = Math.floor(distance / _day);
        var hours = Math.floor((distance % _day) / _hour);
        if (hours < 10) hours = '0' + hours;
        var minutes = Math.floor((distance % _hour) / _minute);
        if (minutes < 10) minutes = '0' + minutes;
        var seconds = Math.floor((distance % _minute) / _second);
        if (seconds < 10) seconds = '0' + seconds;
        document.getElementById('countdown').innerHTML = ""+ days +" дн. "+ hours +":"+ minutes +":"+ seconds +"";
        document.getElementById('countdown2').innerHTML = ""+ days +" дн. "+ hours +":"+ minutes +":"+ seconds +"";

